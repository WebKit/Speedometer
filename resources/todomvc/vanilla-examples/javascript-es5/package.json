{
    "name": "javascript-es5",
    "version": "1.0.0",
    "description": "TodoMVC app written with JavaScript es5 features.",
    "engines": {
        "node": ">=18.13.0",
        "npm": ">=8.19.3"
    },
    "private": true,
    "scripts": {
        "dev": "http-server ./ -p 7001 -c-1 --cors",
        "build": "node scripts/build.js",
        "build:embedded": "node scripts/build-embedded.js",
        "serve": "http-server ./dist -p 7002 -c-1 --cors",
        "serve:embedded": "http-server ./embedded-dist -p 7002 -c-1 --cors"
    },
    "dependencies": {
        "todomvc-app-css": "^2.4.2",
        "todomvc-common": "^1.0.5"
    },
    "devDependencies": {
        "http-server": "^14.1.1",
        "jsdom": "^22.1.0"
    }
}
