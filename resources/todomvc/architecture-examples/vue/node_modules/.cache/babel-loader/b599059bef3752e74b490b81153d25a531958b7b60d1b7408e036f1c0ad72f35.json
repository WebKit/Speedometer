{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { nextTick } from 'vue';\nexport default {\n  data() {\n    return {\n      todos: [],\n      newTodo: '',\n      editing: null\n    };\n  },\n  directives: {\n    todoFocus(el, value) {\n      if (value) {\n        // eslint-disable-next-line no-unused-vars\n        nextTick(_ => {\n          el.focus();\n        });\n      }\n    }\n  },\n  methods: {\n    addTodo() {\n      this.todos.push({\n        completed: false,\n        title: this.newTodo\n      });\n      this.newTodo = '';\n    },\n    deleteTodo(todo) {\n      this.todos = this.todos.filter(t => t !== todo);\n    },\n    deleteCompleted() {\n      this.todos = this.todos.filter(todo => !todo.completed);\n    },\n    editTodo(todo) {\n      this.editing = todo;\n    },\n    doneEdit() {\n      this.editing = null;\n    }\n  },\n  computed: {\n    remaining() {\n      return this.todos.filter(todo => !todo.completed).length;\n    },\n    pluralizedWord() {\n      return this.remaining === 1 ? 'item' : 'items';\n    },\n    completed() {\n      return this.todos.filter(todo => todo.completed).length;\n    },\n    filteredTodos() {\n      if (this.$route.name === 'active') {\n        return this.todos.filter(todo => !todo.completed);\n      } else if (this.$route.name === 'completed') {\n        return this.todos.filter(todo => todo.completed);\n      }\n      return this.todos;\n    },\n    route() {\n      return this.$route.name;\n    },\n    allDone: {\n      get() {\n        return this.remaining === 0;\n      },\n      set(value) {\n        this.todos.forEach(todo => {\n          todo.completed = value;\n        });\n      }\n    }\n  }\n};","map":{"version":3,"names":["nextTick","data","todos","newTodo","editing","directives","todoFocus","el","value","_","focus","methods","addTodo","push","completed","title","deleteTodo","todo","filter","t","deleteCompleted","editTodo","doneEdit","computed","remaining","length","pluralizedWord","filteredTodos","$route","name","route","allDone","get","set","forEach"],"sources":["/Users/thorstenk/Desktop/FlashDesignory/Speedometer/resources/todomvc/architecture-examples/vue/src/components/TodoComponent.vue"],"sourcesContent":["<template>\n    <section class=\"todoapp\">\n        <header class=\"header\">\n            <h1>Todos</h1>\n            <input type=\"text\" class=\"new-todo\" autofocus autocomplete=\"off\" placeholder=\"What needs to be done?\"\n                v-model=\"newTodo\" @keyup.enter=\"addTodo\" />\n        </header>\n        <section class=\"main\" v-show=\"todos.length\" v-cloak>\n            <div class=\"toggle-all-container\">\n                <input type=\"checkbox\" id=\"toggle-all-input\" class=\"toggle-all\" v-model=\"allDone\" />\n                <label class=\"toggle-all-label\" htmlFor=\"toggle-all-input\">\n                    Toggle All Input\n                </label>\n            </div>\n            <ul class=\"todo-list\">\n                <li class=\"todo\" v-bind:key=\"todo.id\" v-for=\"todo in filteredTodos\"\n                    :class=\"{ completed: todo.completed, editing: todo === editing }\">\n                    <div class=\"view\">\n                        <input type=\"checkbox\" v-model=\"todo.completed\" class=\"toggle\">\n                        <label @dblclick=\"editTodo(todo)\">{{ todo.title }}</label>\n                        <button class=\"destroy\" @click.prevent=\"deleteTodo(todo)\"></button>\n                    </div>\n                    <input type=\"text\" class=\"edit\" v-model=\"todo.title\" @keyup.enter=\"doneEdit\" @blur=\"doneEdit\"\n                        v-todoFocus=\"todo === editing\" />\n                </li>\n            </ul>\n        </section>\n        <footer class=\"footer\" v-show=\"todos.length > 0\">\n            <span class=\"todo-count\">\n                <strong>{{ remaining }}</strong> {{ pluralizedWord }} left\n            </span>\n            <ul class=\"filters\">\n                <li><router-link to=\"/\" :class=\"{ selected: route == 'all' }\">All</router-link></li>\n                <li><router-link to=\"/active\" :class=\"{ selected: route == 'active' }\">Active</router-link></li>\n                <li><router-link to=\"/completed\" :class=\"{ selected: route == 'completed' }\">Completed</router-link></li>\n            </ul>\n            <button class=\"clear-completed\" v-show=\"completed\" @click.prevent=\"deleteCompleted\">Clear Completed</button>\n        </footer>\n    </section>\n</template>\n\n<script>\nimport { nextTick } from 'vue';\n\nexport default {\n    data() {\n        return {\n            todos: [],\n            newTodo: '',\n            editing: null\n        }\n    },\n    directives: {\n        todoFocus(el, value) {\n            if (value) {\n                // eslint-disable-next-line no-unused-vars\n                nextTick(_ => {\n                    el.focus()\n                })\n            }\n        }\n    },\n    methods: {\n        addTodo() {\n            this.todos.push({\n                completed: false,\n                title: this.newTodo\n            })\n            this.newTodo = ''\n        },\n        deleteTodo(todo) {\n            this.todos = this.todos.filter(t => t !== todo)\n        },\n        deleteCompleted() {\n            this.todos = this.todos.filter(todo => !todo.completed)\n        },\n        editTodo(todo) {\n            this.editing = todo\n        },\n        doneEdit() {\n            this.editing = null\n        }\n    },\n    computed: {\n        remaining() {\n            return this.todos.filter(todo => !todo.completed).length\n        },\n        pluralizedWord() {\n            return this.remaining === 1 ? 'item' : 'items'\n        },\n        completed() {\n            return this.todos.filter(todo => todo.completed).length\n        },\n        filteredTodos() {\n            if (this.$route.name === 'active') {\n                return this.todos.filter(todo => !todo.completed)\n            } else if (this.$route.name === 'completed') {\n                return this.todos.filter(todo => todo.completed)\n            }\n            return this.todos\n        },\n        route() {\n            return this.$route.name;\n        },\n        allDone: {\n            get() {\n                return this.remaining === 0\n            },\n            set(value) {\n                this.todos.forEach(todo => {\n                    todo.completed = value\n                });\n            }\n        }\n    }\n}\n</script>\n\n<style src=\"./todo-component.css\"></style>\n<style src=\"../../node_modules/todomvc-common/base.css\"></style>\n<style src=\"../../node_modules/todomvc-app-css/index.css\"></style>\n"],"mappings":";AA0CA,SAASA,QAAO,QAAS,KAAK;AAE9B,eAAe;EACXC,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,KAAK,EAAE,EAAE;MACTC,OAAO,EAAE,EAAE;MACXC,OAAO,EAAE;IACb;EACJ,CAAC;EACDC,UAAU,EAAE;IACRC,SAASA,CAACC,EAAE,EAAEC,KAAK,EAAE;MACjB,IAAIA,KAAK,EAAE;QACP;QACAR,QAAQ,CAACS,CAAA,IAAK;UACVF,EAAE,CAACG,KAAK,EAAC;QACb,CAAC;MACL;IACJ;EACJ,CAAC;EACDC,OAAO,EAAE;IACLC,OAAOA,CAAA,EAAG;MACN,IAAI,CAACV,KAAK,CAACW,IAAI,CAAC;QACZC,SAAS,EAAE,KAAK;QAChBC,KAAK,EAAE,IAAI,CAACZ;MAChB,CAAC;MACD,IAAI,CAACA,OAAM,GAAI,EAAC;IACpB,CAAC;IACDa,UAAUA,CAACC,IAAI,EAAE;MACb,IAAI,CAACf,KAAI,GAAI,IAAI,CAACA,KAAK,CAACgB,MAAM,CAACC,CAAA,IAAKA,CAAA,KAAMF,IAAI;IAClD,CAAC;IACDG,eAAeA,CAAA,EAAG;MACd,IAAI,CAAClB,KAAI,GAAI,IAAI,CAACA,KAAK,CAACgB,MAAM,CAACD,IAAG,IAAK,CAACA,IAAI,CAACH,SAAS;IAC1D,CAAC;IACDO,QAAQA,CAACJ,IAAI,EAAE;MACX,IAAI,CAACb,OAAM,GAAIa,IAAG;IACtB,CAAC;IACDK,QAAQA,CAAA,EAAG;MACP,IAAI,CAAClB,OAAM,GAAI,IAAG;IACtB;EACJ,CAAC;EACDmB,QAAQ,EAAE;IACNC,SAASA,CAAA,EAAG;MACR,OAAO,IAAI,CAACtB,KAAK,CAACgB,MAAM,CAACD,IAAG,IAAK,CAACA,IAAI,CAACH,SAAS,CAAC,CAACW,MAAK;IAC3D,CAAC;IACDC,cAAcA,CAAA,EAAG;MACb,OAAO,IAAI,CAACF,SAAQ,KAAM,IAAI,MAAK,GAAI,OAAM;IACjD,CAAC;IACDV,SAASA,CAAA,EAAG;MACR,OAAO,IAAI,CAACZ,KAAK,CAACgB,MAAM,CAACD,IAAG,IAAKA,IAAI,CAACH,SAAS,CAAC,CAACW,MAAK;IAC1D,CAAC;IACDE,aAAaA,CAAA,EA