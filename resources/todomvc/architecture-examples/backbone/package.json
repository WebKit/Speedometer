{
    "name": "backbone",
    "version": "1.0.0",
    "description": "TodoMVC app written with backbone.",
    "engines": {
        "node": ">=18.13.0",
        "npm": ">=8.19.3"
    },
    "private": true,
    "scripts": {
        "build": "node ./standalone/scripts/build.js",
        "build:complex": "node ./complex/scripts/build.js",
        "dev": "http-server ./ -p 7001 -c-1 --cors",
        "serve": "http-server ./standalone/dist -p 7002 -c-1 --cors",
        "serve:complex": "http-server ./complex/dist -p 7002 -c-1 --cors"
    },
    "dependencies": {
        "backbone": "^1.4.1",
        "big-dom-generator": "file:../../big-dom-generator",
        "jquery": "^3.6.4",
        "jsdom": "^22.1.0",
        "todomvc-app-css": "^2.4.2",
        "todomvc-common": "^1.0.5",
        "underscore": "^1.13.6"
    },
    "devDependencies": {
        "http-server": "^14.1.1"
    }
}
