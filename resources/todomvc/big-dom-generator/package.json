{
  "name": "big-dom-generator",
  "version": "1.0.0",
  "description": "Complex DOM generator to embed TodoMVC in a static HTML page",
  "private": true,
  "engines": {
    "node": ">=18.13.0",
    "npm": ">=8.19.3"
  },
  "scripts": {
    "generate": "webpack --config generator.config.js && node dist/app.bundle.js && purgecss -css dist/app.css --content dist/index.html --output=dist/app.css",
    "build": "npm run generate",
    "dev": "webpack serve --open --config webpack.dev.js",
    "serve": "http-server ./dist -p 7002 -c-1 --cors"
  },
