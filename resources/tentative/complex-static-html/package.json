{
  "name": "complex-static-html",
  "version": "1.0.0",
  "description": "Complex DOM generator to embed TodoMVC in a static HTML page",
  "private": true,
  "engines": {
    "node": ">=18.13.0",
    "npm": ">=8.19.3"
  },
  "scripts": {
    "generate-dom": "webpack --config generator.config.js && node generator-dist/app.bundle.js && purgecss -css generator-dist/app.css --content generator-dist/index.html --output=generator-dist/app.css && cp generator-dist/index.html src/react-todomvc/public/index.html && cp generator-dist/app.css src/react-todomvc/public/app.css  && cp generator-dist/logo.png src/react-todomvc/public/logo.png",
    "build-todomvc": "webpack --config webpack.prod.js",
    "build": "npm run generate-dom && npm run build-todomvc",
    "serve": "http-server ./dist -p 7002 -c-1 --cors",
    "serve-gen": "http-server ./generator-dist -p 7002 -c-1 --cors"
  },
  "devDependencies": {
